{
  "name": "sinon-mongo",
  "version": "1.0.1",
  "description": "sinon extensions for unit testing code using the mongodb driver",
  "author": {
    "name": "Daniel Jimenez Garcia",
    "url": "https://github.com/DaniJG"
  },
  "repository": {
    "url": "https://github.com/DaniJG/sinon-mongo"
  },
  "homepage": "https://github.com/DaniJG/sinon-mongo",
  "keywords": [
    "sinon",
    "mongo",
    "mongodb",
    "mock",
